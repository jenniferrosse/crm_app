<div class="container">

<p id="notice"><%= notice %></p>

<h1>Listing Brokers</h1>

    <% @brokers.each do |broker| %>
      
        <%= broker.first_name %>
        <%= broker.last_name %></br>
        <%= broker.email %></br>
        <%= broker.mobile %></br>
        <%= broker.direct_phone %></br>
        <%= broker.company %></br></br>
  
        <%= link_to 'Show', broker %>        <%= link_to 'Edit', edit_broker_path(broker) %>
        <%= link_to 'Destroy', broker, method: :delete, data: { confirm: 'Are you sure?' } %></br></br>
    <% end %>


<br>

<%= link_to 'New Broker', new_broker_path %>
